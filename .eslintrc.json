{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": ["tsconfig.json"]
  },
  "plugins": ["react"],
  "rules": {
    "no-duplicate-imports": 2,
    "no-param-reassign": 2,
    "prefer-const": 2,
    "no-var": 2,
    "import/no-unused-modules": 2,
    "@typescript-eslint/explicit-function-return-type": 0,
    "@typescript-eslint/quotes": [1, "double"]
  }
}
